{
    "name": "jhml/phpexcel",
    "description": "clone of PHPExcel and updated to work with PHP 8.0",
    "keywords": [
        "PHP",
        "Excel",
        "OpenXML",
        "xlsx",
        "xls",
        "spreadsheet"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "https://github.com/jamalludin02/phpexcel",
    "type": "library",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "jhamal"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*"
    },
    "recommend": {
        "ext-zip": "*",
        "ext-gd": "*"
    },
    "autoload": {
        "psr-0": {
            "PHPExcel": "Classes/"
        },
        "psr-4": {
            "PhpOffice\\PhpExcel\\": "Classes/"
        }
    }
}